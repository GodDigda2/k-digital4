    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "# 응답 객체 받아오기\n",
    "response = requests.get(\"https://www.naver.com\")\n",
    "\n",
    "# 객체 안에 html 속성 가져오기\n",
    "html = response.text\n",
    "soup = BeautifulSoup(html, 'html.parser')\n",
    "anchor = soup.select_one(\"#NM_set_home_btn\")\n",
    "anchor.text"
   ]
