   "source": [
    "import openpyxl\n",
    "\n",
    "# 1. 엑셀 파일 생성\n",
    "wb = openpyxl.Workbook()\n",
    "\n",
    "# 2. 워크 시트 생성\n",
    "ws = wb.create_sheet(\"월드컵\")\n",
    "\n",
    "# 3. 데이터 추가\n",
    "ws['A1'] = '선수번호'\n",
    "ws['B1'] = '선수이름'\n",
    "\n",
    "ws['A2'] = '7'\n",
    "ws['B2'] = '손흥민'\n",
    "\n",
    "# 4. 한 행 추가\n",
    "ws.append(['11', '황희찬'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 엑셀 저장\n",
    "wb.save('월드컵.xlsx')"
   ]
  },
